{
  "expo": {
    "name": "Soundico",
    "description": "test",
    "scheme": "soundico",
    "slug": "musiccoapp",
    "privacy": "public",
    "version": "0.0.1",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Soundico to access your photos.",
          "savePhotosPermission": "Allow Soundico to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "githubUrl": "https://github.com/MaisimZaman/Free-Music-Soundify-",
    "orientation": "portrait",
    "icon": "./assets/SoundicoLogo.png",
    "splash": {
      "image": "./assets/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#141414"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSPhotoLibraryUsageDescription": "Allow Soundico to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Soundico to save photos."
      },
      "bundleIdentifier": "com.maisimdevelops.musiccoapp"
    },
    "android": {
      "package": "com.maisimdevelops.musiccoapp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    }
  }
}
